{
  "name": "eoriol-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:universal": "npm run build:client-and-server-bundles && npm run tsc:server",
    "serve:universal": "DISABLE_FIREBASE=true node dist/index.js",
    "build:client-and-server-bundles": "ng build --prod --aot && ng run eoriol-front:server",
    "tsc:server": "tsc -p server/tsconfig.ssr.json",
    "build:universal:debug": "npm run build:client-and-server-debug && npm run tsc:server:debug",
    "build:client-and-server-debug": "ng build --aot && ng build --aot --app 1 --output-hashing=false",
    "tsc:server:debug": "tsc -p server/tsconfig.ssr.json --sourceMap",
    "json-server": "json-server --watch db.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-lite/json-ld": "^0.6.2",
    "@nicky-lenaers/ngx-scroll-to": "^3.0.1",
    "angulartics2": "^8.3.0",
    "code-prettify": "^0.1.0",
    "compression": "^1.7.2",
    "core-js": "^2.5.7",
    "express": "^4.17.3",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "ngx-infinite-scroll": "^8.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.4.0",
    "ts-loader": "^3.5.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^13.9.1",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "^3.4.5"
  }
